                <label for="dataZwrotu">data zwrotu:<sup>*</sup></label>
                <input type="date" name="dataZwrotu" id="dataZwrotu" value="2020-10-22" disabled/>
                <span id="errorDataZwrotu" class="errors-text"></span>
           
                <label for="przejechanyDystans">przejechany dystans:<sup>*</sup></label>
                <input type="number" name="przejechanyDystans" id="przejechanyDystans" value="100" disabled/>
                <span id="errorPrzejechanyDystans" class="errors-text"></span>
                
                <a href="rent-edit.html" class="details-actions-button-edit">Edytuj</a> 
                <a href="/rents" class="details-actions-button-return">Powrót</a>  
                
